{
  "name": "funsdk",
  "version": "0.2.1",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "description": "A strong-typed pumpfun SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
